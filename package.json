{
  "name": "sprint-app",
  "displayName": "Sprint App",
  "version": "0.0.1",
  "publisher": "sprint-ai",
  "description": "Sprint App VSCode Extension",
  "engines": {
    "vscode": "^1.74.0"
  },
  "scripts": {
    "clean": "node --max-old-space-size=4096 -e \"const fs = require('fs'); if (fs.existsSync('dist')) fs.rmSync('dist', { recursive: true });\"",
    "prebuild": "npm run clean",
    "build": "npm run build:cjs",
    "build:esm": "node esbuild.cjs",
    "build:cjs": "node esbuild.cjs --cjs",
    "pretest": "npm run build:esm",
    "test": "vitest run",
    "test:watch": "vitest",
    "vscode:prepublish": "npm run build:cjs",
    "watch": "node esbuild.cjs --watch"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onView:sprint-sidebar-view"
  ],
  "main": "./dist/extension.cjs",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sprint-sidebar",
          "title": "Sprint App",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "sprint-sidebar": [
        {
          "type": "webview",
          "id": "sprint-sidebar-view",
          "name": "Sprint App",
          "icon": "media/icon.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "sprint-ai.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "sprint-ai.showSidebar",
        "title": "Show Sprint App Sidebar"
      }
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "concurrently": "^9.1.2",
    "esbuild": "^0.24.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-vitest": "^0.5.4",
    "esquery": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.6",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "rxjs": "^7.8.1"
  }
}
